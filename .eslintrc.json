{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "react-hooks/exhaustive-deps": "off",
    "@next/next/no-sync-scripts": "off",
    "comma-dangle": "off",
    "@typescript-eslint/comma-dangle": "off",
    "no-unused-vars": 2,
    "prettier/prettier": "error"
  }
}
